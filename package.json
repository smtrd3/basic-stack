{
  "name": "mini-stack",
  "version": "0.0.0",
  "private": true,
  "description": "Super basic vite based fullstack template with HMR",
  "type": "module",
  "scripts": {
    "dev": "bun --bun vite",
    "build": "bun --bun vite build --app",
    "start": "bun run build && bun --bun ./dist",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check ."
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "hono": "^4.9.9",
    "mutative": "^1.2.0",
    "nanoid": "^5.1.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "use-mutative": "^1.3.0",
    "wouter": "^3.7.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@vitejs/plugin-react": "^5.0.0",
    "oxfmt": "^0.20.0",
    "srvx": "^0.8.9",
    "tailwindcss": "^4.1.13",
    "vite": "npm:rolldown-vite@latest"
  },
  "packageManager": "bun@1.2.0"
}
